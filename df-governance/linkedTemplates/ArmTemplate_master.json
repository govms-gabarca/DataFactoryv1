{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-governance"
		},
		"Lnk_dlsawgovernance_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Lnk_dlsawgovernance'"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-governancepbi.vault.azure.net/"
		},
		"Lnk_PowerBI_API_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://wabi-south-central-us-redirect.analysis.windows.net/v1.0/myorg/admin"
		},
		"Lnk_PowerBI_API_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "acdd70b8-1d65-4e11-b26f-adbb4847427a"
		},
		"Lnk_PowerBI_API_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "5a4c51fd-c59d-493e-8aec-8f6300364946"
		},
		"Lnk_PowerBI_API_properties_typeProperties_aadResourceId": {
			"type": "string",
			"defaultValue": "https://analysis.windows.net/powerbi/api"
		},
		"Lnk_Powerbi_Actividades_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.powerbi.com/v1.0/myorg/admin/activityevents"
		},
		"Lnk_Powerbi_Actividades_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "acdd70b8-1d65-4e11-b26f-adbb4847427a"
		},
		"Lnk_Powerbi_Actividades_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "5a4c51fd-c59d-493e-8aec-8f6300364946"
		},
		"Lnk_Powerbi_Actividades_properties_typeProperties_aadResourceId": {
			"type": "string",
			"defaultValue": "https://analysis.windows.net/powerbi/api"
		},
		"Lnk_dlsawgovernance_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlsgovernance.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-governance_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Lnk_dlsawgovernance_accountKey": {
						"value": "[parameters('Lnk_dlsawgovernance_accountKey')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"Lnk_dlsawgovernance_properties_typeProperties_url": {
						"value": "[parameters('Lnk_dlsawgovernance_properties_typeProperties_url')]"
					},
					"Lnk_PowerBI_API_properties_typeProperties_url": {
						"value": "[parameters('Lnk_PowerBI_API_properties_typeProperties_url')]"
					},
					"Lnk_PowerBI_API_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('Lnk_PowerBI_API_properties_typeProperties_servicePrincipalId')]"
					},
					"Lnk_PowerBI_API_properties_typeProperties_tenant": {
						"value": "[parameters('Lnk_PowerBI_API_properties_typeProperties_tenant')]"
					},
					"Lnk_PowerBI_API_properties_typeProperties_aadResourceId": {
						"value": "[parameters('Lnk_PowerBI_API_properties_typeProperties_aadResourceId')]"
					},
					"Lnk_Powerbi_Actividades_properties_typeProperties_url": {
						"value": "[parameters('Lnk_Powerbi_Actividades_properties_typeProperties_url')]"
					},
					"Lnk_Powerbi_Actividades_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('Lnk_Powerbi_Actividades_properties_typeProperties_servicePrincipalId')]"
					},
					"Lnk_Powerbi_Actividades_properties_typeProperties_tenant": {
						"value": "[parameters('Lnk_Powerbi_Actividades_properties_typeProperties_tenant')]"
					},
					"Lnk_Powerbi_Actividades_properties_typeProperties_aadResourceId": {
						"value": "[parameters('Lnk_Powerbi_Actividades_properties_typeProperties_aadResourceId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}